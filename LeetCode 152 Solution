//SOLUTION 1


class Solution {
    public int maxProduct(int[] a) 
    {
        int n=a.length, m=a[0];
        for(int i=0;i<n;i++)
        {
            int p=1;
            for(int j=i;j<n;j++)
            {
                p*=a[j];
                if(p>m) 
                m=p;
            }
        }
        return m;
    }
}
