//SOLUTION 1

class Solution
{
    public int majorityElement(int[] a) 
{
        int n=a.length;
        for(int i=0;i<n;i++){
            int c=0;
            for(int j=0;j<n;j++){
                if(a[j]==a[i]) c++;
            }
            if(c>n/2) return a[i];
        }
        return -1;
    }
}
