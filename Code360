//SOLUTION 1

import java.util.* ;
import java.io.*; 
public class Solution 
{
    public static void reverseArray(ArrayList<Integer> a, int m)
    {
        int i=m+1,j=a.size()-1;
        while(i<j){
            int t=a.get(i);
            a.remove(i);
            a.add(i,a.get(j-1));
            a.remove(j);
            a.add(j,t);
            i++;
            j--;
        }
    }
}
