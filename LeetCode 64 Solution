//SOLUTION 1

class Solution {
    public int minPathSum(int[][] g) {
        int m = g.length, n = g[0].length;
        for (int i = 0; i < m; i++)
            for (int j = 0; j < n; j++) {
                if (i == 0 && j == 0) continue;
                int top = i > 0 ? g[i - 1][j] : Integer.MAX_VALUE;
                int left = j > 0 ? g[i][j - 1] : Integer.MAX_VALUE;
                g[i][j] += Math.min(top, left);
            }
        return g[m - 1][n - 1];
    }
}
