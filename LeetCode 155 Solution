//SOLUTION 1

class MinStack {
    int[] a=new int[100000];
    int[] b=new int[100000];
    int top=-1;
    public MinStack() {}
    public void push(int x) {
        a[++top]=x;
        if(top==0)b[top]=x;
        else b[top]=Math.min(b[top-1],x);
    }
    public void pop() {
        top--;
    }
    public int top() {
        return a[top];
    }
    public int getMin() {
        return b[top];
    }
}
