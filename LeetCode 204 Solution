//SOLUTION 1

class Solution {
    public int countPrimes(int n) {
        if (n < 3) return 0;
        boolean[] p = new boolean[n];
        int c = 0;
        for (int i = 2; i * i < n; i++)
            if (!p[i])
                for (int j = i * i; j < n; j += i)
                    p[j] = true;
        for (int i = 2; i < n; i++)
            if (!p[i]) c++;
        return c;
    }
}
