//SOLUTION  1

class Solution {
    public boolean searchMatrix(int[][] m, int t) {
        int r = m.length, c = m[0].length;
        int i = 0, j = c - 1;
        while(i < r && j >= 0) {
            if(m[i][j] == t) return true;
            if(m[i][j] > t) j--;
            else i++;
        }
        return false;
    }
}
